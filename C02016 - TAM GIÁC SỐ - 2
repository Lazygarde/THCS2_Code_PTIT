#include <stdio.h>
int main()
{
    int n;
    scanf("%d", &n);
    for (int i = 1; i <= n; i++)
    {
        int k;
        if (i % 2 == 1)
            k = 1;
        else
            k = 2;
        for (int j = 1; j <= i; j++)
        {
            printf("%d", k);
            k += 2;
        }
        printf("\n");
    }
}
