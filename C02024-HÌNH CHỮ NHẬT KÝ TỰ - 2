#include <stdio.h>
int main(){
	int m,n,k=0;
	scanf("%d%d",&m,&n);
	if(m<=n){
		for(int i=1;i<=m;i++){
			for(int j=k;j<n;j++){
				printf("%c",'A'+j);
			}
			for(int j=k-1;j>=0;j--){
				printf("%c",'A'+j);
			}
			k++;
			printf("\n");
		}
	}
	else{
		for(int i=0;i<n;i++){
			for(int j=k;j<n;j++){
				printf("%c",'A'+j);
			}
			for(int j=k-1;j>=0;j--){
				printf("%c",'A'+j);
			}
			k++;
			printf("\n");
		}
		for(int i=n;i<m;i++){
			for(int j=n;j>0;j--){
				printf("%c",'A'+j-1);
			}
			printf("\n");
		}
		
	}
}
