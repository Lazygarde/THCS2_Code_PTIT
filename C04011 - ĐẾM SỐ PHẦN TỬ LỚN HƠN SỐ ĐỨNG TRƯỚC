#include <stdio.h>
int main()
{
    int t;
    scanf("%d", &t);
    while (t--)
    {
        int n, x, s = 0, max = 0;
        scanf("%d", &n);
        for (int i = 1; i <= n; i++)
        {
            scanf("%d", &x);
            if (x > max)
            {
                s++;
                max = x;
            }
        }
        printf("%d\n", s);
    }
}
