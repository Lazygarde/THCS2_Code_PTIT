#include <stdio.h>
int main()
{
    int t;
    scanf("%d", &t);
    for (int z = 1; z <= t; z++)
    {
        int n, m, k;
        scanf("%d%d%d", &n, &m, &k);
        int a[n + 1], b[m + 1];
        for (int i = 1; i <= n; i++)
            scanf("%d", &a[i]);
        for (int i = 1; i <= m; i++)
            scanf("%d", &b[i]);
        printf("Test %d:\n", z);
        for (int i = 1; i <= k; i++)
            printf("%d ", a[i]);
        for (int i = 1; i <= m; i++)
            printf("%d ", b[i]);
        for (int i = k + 1; i <= n; i++)
            printf("%d ", a[i]);
        printf("\n");
    }
}
